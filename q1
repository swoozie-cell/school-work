#include <iostream>
using namespace std;

int main() {
	int port ;
	
	cout<<"enter port number : " ;
	cin >> port;
	
	if ((port >= 14 && port <= 72) || (port > 103)) {
        cout << "within range" ;
    } else {
        cout << "out of range" ;
    }
	
	


    return 0;
}
